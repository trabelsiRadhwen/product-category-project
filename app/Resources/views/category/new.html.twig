<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Ajouter categorie{% endblock %}</title>
    {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('bootstrap4/bootstrap.min.css') }}">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    {% endblock %}
</head>
<body>

{% block body %}

    <div class="container">
        {{ form_start(form) }}

        {{ form_row(form.category) }}

        <button type="submit">Ajouter</button>
        {{ form_end(form) }}
    </div>
{% endblock %}

{% block javascripts %}
    <script src="{{ asset('bootstrap4/bootstrap.min.js') }}"></script>
{% endblock %}

</body>
</html>